<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/Stylesheets/stylesheet.css" rel="stylesheet" />
    <script src="~/Content/Scripts/jquery-1.5.2.min.js"></script>
    <script src="~/Content/Scripts/widgets.js"></script>
    <style type="text/css">
        body {
            background-color: #;
            font-family: Tahoma;
            color: #000000;
        }

        #container_boxed {
            background-color: #;
        }

        h1, h2, h3, h4 {
            font-family: 'Ubuntu Condensed';
            color: #;
        }

        .product-info .pricetag .new_price, .product-info .pricetag .promo_price, .info2 .price, .product_unit .price, .prod_hold .new_price, .product-info .new_price, .roundabout-moveable-item .pricetag, .roundabout-moveable-item div.prod_holder h3, .info .price2, .info .price {
            font-family: 'Ubuntu Condensed';
        }

        #container_boxed h1.heading-title span, #container_boxed h2.heading-title span {
            background-image: none;
        }

        #column-left .box .box-content, #column-right .box .box-content {
            background-color: #;
        }

        #column-left h3.heading-title span, #column-right h3.heading-title span {
            color: #;
        }

        a, a:visited {
            color: #;
        }

            a:hover, #footer ul li a:hover {
                color: #;
            }

        #footer h3 {
            color: #;
            border-bottom: 1px solid #;
        }

        #header {
            color: #;
            background-color: #;
        }

        #content_holder {
            background-color: #;
        }

        #footer {
            background-color: #;
        }

            #footer ul li {
                border-right: 1px solid #;
            }

            #footer .column_small, #facebook_holder, #footer .column_big {
                border-bottom: 1px solid #;
            }

            #footer ul li a {
                color: #;
            }

        ul.main_menu li a, ul.main_menu li.subico span {
            color: #;
        }

        #menu_holder {
            background-color: #;
        }

        #pre_footer {
            background-color: #;
        }

        .menu ul#topnav li a {
            color: #;
        }

        .product-info .old_price {
            color: #;
        }

        .product-info .product_price_sale .new_price, .product-info .new_price {
            color: #;
        }

        a.button {
            color: #F9F9F9;
            background-color: #;
        }

        a#button-cart {
            background-color: #;
        }
    </style>
</head>
<body>
    <div id="container">
        @RenderBody()
    </div>
</body>
</html>
